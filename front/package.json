{
  "name": "front",
  "description": "A Vue.js project",
  "version": "1.0.0",
  "author": "mouadhkaabachi <mouadh.kaabachi@esprit.tn>",
  "license": "MIT",
  "private": true,
  "scripts": {
    "dev": "cross-env NODE_ENV=development webpack-dev-server --hot",
    "build": "cross-env NODE_ENV=production webpack --progress --hide-modules",
